<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5.0-windows</TargetFramework>
    <AssemblyName>Headless</AssemblyName>
    <RootNamespace>Headless</RootNamespace>
    <Description>A super basic example of headless execution with Surreal.</Description>
  </PropertyGroup>
  <Import Project="..\..\Shared.props" />
  <Import Project="..\..\Executables.props" />
  <ItemGroup>
    <ProjectReference Include="..\..\modules\Surreal.Modules.Jam\Surreal.Modules.Jam.csproj" />
    <ProjectReference Include="..\..\platforms\Surreal.Platform.Headless\Surreal.Platform.Headless.csproj" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="$(ProjectDir)\Resources\**" />
    <Content Include="$(ProjectDir)\Assets\**" CopyToOutputDirectory="PreserveNewest" />
  </ItemGroup>
</Project>