<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:surreal="clr-namespace:Surreal.Editing"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:CompileBindings="True"
        x:Class="Surreal.Editing.MainWindow"
        x:DataType="surreal:EditorApplication"
        Width="{Binding Configuration.DefaultWidth, Mode=OneTime}"
        Height="{Binding Configuration.DefaultHeight, Mode=OneTime}"
        Title="{Binding  Configuration.WindowTitle}">
</Window>
